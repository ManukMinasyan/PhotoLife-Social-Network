{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "anlutro/l4-settings": "^0.11.0",
    "beyondcode/laravel-websockets": "^1.3",
    "dflydev/apache-mime-types": "^1.0",
    "fideloper/proxy": "^4.0",
    "glorand/laravel-model-settings": "^3.5",
    "intervention/image": "^2.5",
    "laravel/framework": "^6.5.2",
    "laravel/passport": "^7.5",
    "laravel/telescope": "^2.1",
    "laravel/tinker": "^1.0",
    "mad-web/laravel-initializer": "^2.0",
    "overtrue/laravel-follow": "^1.1",
    "php-ffmpeg/php-ffmpeg": "^0.14.0",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.2",
    "spatie/laravel-medialibrary": "^8.2.1",
    "spatie/laravel-tags": "^2.5",
    "fzaninotto/faker": "^1.4"
  },
  "require-dev": {
    "facade/ignition": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "poteto-dev/laravel-ui-stisla": "^0.1.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "app-install": [
      "@composer install",
      "@php artisan app:install"
    ],
    "app-update": [
      "@composer install",
      "@php artisan app:update"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}