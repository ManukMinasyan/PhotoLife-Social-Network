{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "beyondcode/laravel-websockets": "^1.3",
        "dflydev/apache-mime-types": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "glorand/laravel-model-settings": "^4.4.1",
        "guzzlehttp/guzzle": "^7.4.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^v10.2.2",
        "laravel/telescope": "^v4.6.10",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "mad-web/laravel-initializer": "^3.3.1",
        "overtrue/laravel-follow": "^1.1",
        "php-ffmpeg/php-ffmpeg": "^v0.19.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "spatie/laravel-medialibrary": "^8.2.7",
        "spatie/laravel-tags": "^4.2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "poteto-dev/laravel-ui-stisla": "^0.1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "app-install": [
            "@composer install",
            "@php artisan app:install"
        ],
        "app-update": [
            "@composer install",
            "@php artisan app:update"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
